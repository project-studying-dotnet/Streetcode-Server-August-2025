{
  "ConnectionStrings": {
    "DefaultConnection": "YOUR_CONNECTION_STRING"
  },
  "EmailConfiguration": {
    "From": "YOUR_EMAIL",
    "To": "RECEIVER_EMAIL",
    "SmtpServer": "smtp.gmail.com",
    "Port": 465,
    "Username": "YOUR_EMAIL"
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Blob": {
    "BlobStoreKey": "YOUR_BLOBSTOREKEY",
    "BlobStorePath": "../../BlobStorageFolder/"
  },
  "Payment": {
    "Token": "YOUR_PAYMENT_TOKEN"
  },
  "Instagram": {
    "InstagramID": "YOUR_INSTAGRAM_ID",
    "InstagramToken": "YOUR_INSTAGRAM_TOKEN"
  },
  "CORS": {
    "AllowedOrigins": [],
    "AllowedHeaders": [],
    "AllowedMethods": [],
    "PreflightMaxAge": 1
  }
}
